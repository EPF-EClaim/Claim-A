<SideNavigation xmlns="sap.tnt" xmlns:core="sap.ui.core" 
				expanded="true"
				itemPress=".onItemPress"
				selectedKey=""
				itemSelect=".onItemSelect">
				<tnt:NavigationList>
					<tnt:NavigationListItem
						key="dashboard"
						text="{i18n>dashboard}"
						icon="sap-icon://home"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
                    <tnt:NavigationListItem
						key="createreport"
						text="{i18n>createreport}"
						icon="sap-icon://add"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
                    <tnt:NavigationListItem
						key="myrequest"
						text="{i18n>myrequest}"
						icon="sap-icon://activity-items"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
                    <tnt:NavigationListItem
						key="approval"
						text="{i18n>approvals}"
						icon="sap-icon://sys-enter"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
                    <tnt:NavigationListItem
						key="analytics"
						text="{i18n>analytics}"
						icon="sap-icon://business-objects-experience"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
                    <tnt:NavigationListItem
						key="configuration"
						text="{i18n>configuration}"
						icon="sap-icon://action-settings"
						enabled="true"
						expanded="false">
					</tnt:NavigationListItem>
				</tnt:NavigationList>
			</SideNavigation>